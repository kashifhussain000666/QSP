Option Compare Database
Option Explicit

Dim iFolderID As Long

Private Sub Form_BeforeInsert(Cancel As Integer)
    
    If Nz(iFolderID, 0) = 0 Then
        Cancel = True
    Else
        Me!FolderID = iFolderID
        Me!DateAdded = GetDataCenterDateTime
        Me!UserIDAdded = App.Username
    End If
    
End Sub

Public Sub Parent_Current(ParentID As Long)
    iFolderID = ParentID
    Set Me.Recordset = DBConnection.ExecuteQuery("SELECT * FROM tblFolderFilePattern WHERE FolderID=" & ParentID, False, , ConnectedRSEnum.Connected)
End Sub

Private Sub Form_Load()
    Set cboFilePattern.Recordset = DBConnection.ExecuteQuery("SELECT FilePatternID, FilePatternDesc, FilePattern FROM tblFilePattern ORDER BY FilePatternDesc")
End Sub

Private Sub Form_BeforeDelConfirm(Cancel As Integer, response As Integer)
    ' Suppress default Delete Confirm dialog box.
    response = acDataErrContinue
End Sub

Private Sub Form_Delete(Cancel As Integer)
    If MsgBox("Are you sure to delete the record?", vbYesNo, "Delete Confirmation") = vbNo Then Cancel = True
End Sub
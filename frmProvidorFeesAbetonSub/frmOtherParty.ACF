Private Function appendStr(value1 As String, value2 As String) As String
    'Possibly move this to a module since it is duplicated in two places?
    appendStr = Trim(value1)
    If appendStr <> "" Then
        appendStr = appendStr + " "
    End If
    appendStr = appendStr + Trim(value2)
End Function

Private Function autoFillPrintOnCheckAsValue(sCompanyName As String) As String

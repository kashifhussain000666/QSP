Err_Handler:
    SaveData = False
    If Err.Number = 2501 Then 'DoCmd Cancelled (Validation Failure)
    Else
        MsgBox "Problem saving record: " & Err.description & "(" & Err.Number & ")"
    End If
    Resume Exit_Handler

End Function


Private Sub btnDelete_Click()
On Error GoTo Err_Handler

    If Not GetUserSecurity("delotherparty") Then
        MsgBox (App.Username & " is not authorized to delete other parties.  Please see your system administrator")
        GoTo Exit_Handler
    End If

    If IsNull(Me.cboFind) Then
        MsgBox ("Must find a record before you can delete")
        GoTo Exit_Handler
    End If

    'Check if any case exist
    Dim rsCase As ADODB.Recordset
    Dim strSQL As String
    strSQL = "select count(*) as nbrofcases from tblcaseotherparty where opcode = " & Me!doctorcode
    Set rsCase = DBConnection.ExecuteQuery(strSQL)
    If rsCase("nbrofcases") <> 0 Then
        MsgBox ("Cannot delete this other party.  It is currently assigned to " & rsCase("nbrofcases") & " cases.")
        GoTo Exit_Handler
    End If
    
    'Confirmation
    Dim msg, style, title, response
    msg = "Are you sure you want to delete this record?"
    style = vbYesNo + vbExclamation + vbDefaultButton1
    title = "Delete Confirmation"
    
    response = MsgBox(msg, style, title)
    If response = vbNo Then
        GoTo Exit_Handler
    End If
    
    ' clean up data in child tables - offices
    strSQL = "DELETE FROM tblDoctorOffice WHERE DoctorCode = " & Nz(Me!doctorcode, 0)
    DBConnection.ExecuteNonQuery (strSQL)
    
    'delete record
    DoCmd.DoMenuItem acFormBar, acEditMenu, 8, , acMenuVer70
    DoCmd.DoMenuItem acFormBar, acEditMenu, 6, , acMenuVer70

    Me.cboFind = Null
    Call initFindControl
    
    Call bindData(0)

    Call setUIStates

Exit_Handler:
    Exit Sub

Err_Handler:
    If Err.Number = 2501 Then 'docmd cancelled
        Debug.Print Err.description
    Else
        MsgBox "Problem deleting other party: " & Err.description & "(" & Err.Number & ")"
    End If
    Resume Exit_Handler

End Sub


        If Me.dirty Then
            If SaveData() = False Then
                GoTo Exit_Handler
            End If
        End If
    
        'Bind form to blank record
        Me.cboFind = Null
        Call bindData(0)
        DoCmd.GoToRecord , , acNewRec
        
        Call setUIStates
        
        Me.companyname.setFocus

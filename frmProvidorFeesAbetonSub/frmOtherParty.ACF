Private Function IsValid() As Boolean
On Error GoTo Err_Handler

Dim strErrorMsg As String

    If IsNull(Me.type) Or Len(Me.type) = 0 Then
        strErrorMsg = "Other party type is required"
        Me.type.setFocus
    ElseIf IsNull(Me.companyname) Or Len(Me.companyname) = 0 Then
        strErrorMsg = "Company is required"
        Me.companyname.setFocus
    ElseIf IsNull(Me.addr1) Or Len(Me.addr1) = 0 Then
        strErrorMsg = "Address Line 1 is required"
        Me.addr1.setFocus
    ElseIf IsNull(Me.city) Or Len(Me.city) = 0 Then
        strErrorMsg = "City is required"
        Me.city.setFocus
    ElseIf IsNull(Me.state) Or Len(Me.state) = 0 Then
        strErrorMsg = "State/Province is required"
        Me.state.setFocus
    ElseIf IsNull(Me.zip) Or Len(Me.zip) = 0 Then
        strErrorMsg = "Zip/Postal code is required"
        Me.zip.setFocus
    ElseIf IsNull(Me.status) Or Len(Me.status) = 0 Then
        strErrorMsg = "Status is required"
        Me.status.setFocus
    ElseIf IsNull(Me.prodcodecombo) And Me!CreateInvoices = -1 Then
        strErrorMsg = "You must enter a product if you are going to create invoices transactions."
        Me.prodcodecombo.setFocus
    ElseIf IsNull(Me.prodcodecombo) And Me!CreateVouchers = -1 Then
        strErrorMsg = "You must enter a product if you are going to create vouchers."
        Me.prodcodecombo.setFocus
    End If
    
    If Len(strErrorMsg) = 0 Then
        IsValid = True
    Else
        IsValid = False
        MsgBox strErrorMsg
    End If

Exit_Handler:
    Exit Function

Err_Handler:
    MsgBox Err.description, vbOKOnly + vbCritical, "Error"
    Resume Exit_Handler

End Function


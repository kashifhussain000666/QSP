Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Dim blnIsValid

Private Sub Form_BeforeInsert(Cancel As Integer)
    Me.DateAdded = DateTimeFunc.GetOfficeDateTime(App.CurrentOfficeCode)
    Me.UserIDAdded = App.Username
    Me!OfficeCode = App.CurrentOfficeCode
End Sub

Private Sub CheckRequiredFields()
On Error GoTo Err_Handler
    'Required fields are Phone, email and Priority
    
    If Nz(Me.txtphone, "") = "" Then
        MsgBox "A phone number is required."
        Me.txtphone.setFocus
        blnIsValid = False
        GoTo Exit_Handler
    End If
    
    If Nz(Me.txtemail, "") = "" Then
        MsgBox "An Email is required."
        Me.txtemail.setFocus
        blnIsValid = False
        GoTo Exit_Handler
    End If
    
    If Nz(Me.txtPriority, 0) = 0 Then
        MsgBox "A priority is required."
        Me.txtPriority.setFocus
        blnIsValid = False
        GoTo Exit_Handler
    End If
    
    
Exit_Handler:
    On Error Resume Next
    Exit Sub
Err_Handler:
    Debug.Print Err.description
    Resume Exit_Handler
End Sub


Private Sub CheckForDuplicateEntries()
On Error GoTo Err_Handler
Dim strSQL As String
Dim rsContacts As ADODB.Recordset
    
    strSQL = "SELECT * FROM tblOfficeContact WHERE OfficeCode = " & App.CurrentOfficeCode

    If Nz(Me.cboDepartment, "") = "" Then
        strSQL = strSQL & " AND Department IS NULL"
    Else
        strSQL = strSQL & " AND Department = " & Me.cboDepartment
    End If
        
    If Nz(Me.cboLineOfBusiness, "") = "" Then
        strSQL = strSQL & " AND EWBusLineID IS NULL"
    Else
        strSQL = strSQL & " AND EWBusLineID = " & Me.cboLineOfBusiness
    End If
        
    Set rsContacts = DBConnection.ExecuteQuery(strSQL)
        
    If Not rsContacts.EOF Then
        If rsContacts.RecordCount > 1 Then
            MsgBox "Cannot have duplicates for Office, Department, and Line Of Business."
            blnIsValid = False
            GoTo Exit_Handler
        ElseIf rsContacts("OfficeContactCode") <> Me!OfficeContactCode Then
            MsgBox "Cannot have duplicates for Office, Department, and Line Of Business."
            blnIsValid = False
            GoTo Exit_Handler
        End If
    End If

Exit_Handler:
    On Error Resume Next
    Exit Sub
Err_Handler:
    Debug.Print Err.description
    Resume Exit_Handler
End Sub

Private Sub Form_BeforeUpdate(Cancel As Integer)
    blnIsValid = True
    Call CheckRequiredFields
    If blnIsValid Then Call CheckForDuplicateEntries
    If blnIsValid Then
        Me.DateEdited = DateTimeFunc.GetOfficeDateTime(App.CurrentOfficeCode)
        Me.UserIDEdited = App.Username
    Else
        Cancel = True
    End If
    
End Sub

Private Sub Form_Load()
    If Not App.IsAdmin Then
        Me.AllowEdits = False
        Me.AllowAdditions = False
        Me.AllowDeletions = False
    End If
    
    Set cboDepartment.Recordset = DBConnection.ExecuteQuery("SELECT Value, SubCategory FROM tblCodes WHERE Category = 'OfficeContactDeptCombo' ORDER BY CodeID")
    
    Set Me.Recordset = DBConnection.ExecuteQuery("SELECT * FROM tblOfficeContact WHERE OfficeCode = " & App.CurrentOfficeCode & " ORDER BY Priority", False, , ConnectedRSEnum.Connected)
    
    Set Me.cboLineOfBusiness.Recordset = CbxQuery.LoadTblEWBusLine
    
End Sub

Private Sub Form_BeforeDelConfirm(Cancel As Integer, response As Integer)
    ' Suppress default Delete Confirm dialog box.
    response = acDataErrContinue
End Sub

Private Sub Form_Delete(Cancel As Integer)
    If MsgBox("Are you sure to delete the record?", vbYesNo, "Delete Confirmation") = vbNo Then Cancel = True
End Sub